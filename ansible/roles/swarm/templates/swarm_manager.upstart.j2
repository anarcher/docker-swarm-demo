description "Docker Swarm Master"
author "anarcher"
console log
start on filesystem and started consul
stop on runlevel [!2345]
respawn
script
/usr/local/bin/swarm manage \
    -H tcp://0.0.0.0:{{ swarm_manager_port }} \
    --strategy {{  packing_strategy }} \
    {{ swarm_discovery_string }}
end script

